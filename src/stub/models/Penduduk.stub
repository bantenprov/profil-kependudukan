<?php

namespace App;

use Illuminate\Database\Eloquent\Model;

class Penduduk extends Model
{
    
    protected $table = "penduduks";

    protected $fillable = ['user_id',
    						'nama_lengkap',
    						'jenis_kelamin',
    						'agama',
    						'pekerjaan',
    						'status_perkawinan',
    						'alamat'
    					];


    public function getUser()
	{
		return $this->belongsTo('App\User','user_id','id');	
	}

}
